<html>
    <head>
      <title>Mini Message Board</title>
        <link rel="stylesheet" href="styles.css">
    </head>
    <body>
        <div>
            <% for (let i = 0; i < messages.length; i++) { %>
              <p><%- messages[i].text %></p>
              <p>From: <%= messages[i].user %></p>
              <p>On: <%= messages[i].added %></p>
            <% } %>
          </div>
          <a href="/new">Create new message</a>
    </body>
  </html>